class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        ss=""
        c=0
        for i in s:
            if i not in ss:
                ss+=i
                c=max(c,len(ss))
            else:
                idx=ss.index(i)
                ss=ss[idx+1:]+i
                
        return c
